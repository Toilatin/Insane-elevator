local kavoUi = loadstring(game:HttpGet("https://raw.githubusercontent.com/Toilatin/KavoUI/main/KavoUi?token=GHSAT0AAAAAACGUT5VDUNKGBLPID7H6MBGMZHFZ6TA"))()
local window = kavoUi.CreateLib("Sword Warrior Endless Safe Mode","BloodTheme")

---Hi

local Tab1 = window:NewTab("Endless SafeSpot")
local Tab1Section = Tab1:NewSection("Endless SafeSpot")

local Tab2 = window:NewTab("Scripts")
local Tab2Section = Tab2:NewSection("Scripts")

local Tab3 = window:NewTab("Credits")
local Tab3Section = Tab3:NewSection("-----Credits-----")

--

Tab1Section:NewButton("Place SafeSpot","Place SafeSpot",function()

local SafeSpot = Instance.new("Part", workspace)

SafeSpot.Position = Vector3.new(2334.61,39,-360.65)

SafeSpot.Name = "Safe"

SafeSpot.Size = Vector3.new(2050,1,2050)

SafeSpot.Anchored = true

SafeSpot.Transparency = .7

end) 

Tab1Section:NewButton("Tp To SafeSpot (Cant die by mobs)","Teleport you to safespot (Cant die by mobs)",function()

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Safe.CFrame * CFrame.new(340,50,0)
end)

Tab2Section:NewButton("Infinite Yield","Op FE Admin Commands",function()

loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

